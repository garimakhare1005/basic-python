        "def CheckNumbers(n):\n",
        "  if (n%2) == 0:\n",
        "    print(\"The number is even\")\n",
        "\n",
        "  else:\n",
        "    print(\"The number is odd\")\n",
        "n = float(input())\n",
        "CheckNumbers(n)"
