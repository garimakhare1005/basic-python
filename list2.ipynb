      "cell_type": "code",
      "source": [
        "def swapElements(list,pos1,pos2):\n",
        "  list[pos1],list[pos2] = list[pos2],list[pos1]\n",
        "  return list\n",
        "list = [1,2,3,4,5,6,42,12]\n",
        "pos1,pos2 = 5,2\n",
        "\n",
        "print(swapElements(list,pos1,pos2))\n",
        "print(swapElements(list,pos1-1,pos2-1))"
      ],
