        "def CheckNumbers(n):\n",
        "  if (n%4) == 0:\n",
        "    print(\"The Year is a leap year\")\n",
        "  elif (n % 100) == 0:\n",
        "    print(\"The year is a leap year\")\n",
        "\n",
        "  else:\n",
        "    print(\"The year is NOT a leap year\")\n",
        "n = int(input())\n",
        "CheckNumbers(n)"
      ],
